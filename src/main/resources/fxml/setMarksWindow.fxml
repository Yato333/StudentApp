<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<!--suppress JavaFxDefaultTag -->
<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="TOP_CENTER" VBox.vgrow="NEVER">
         <children>
            <Label text="Set marks for ">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label text="{date}">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <TableView prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn editable="false" maxWidth="50.0" minWidth="16.0" prefWidth="65.0" sortable="false" />
          <TableColumn editable="false" maxWidth="50.0" minWidth="16.0" prefWidth="172.0" sortable="false" />
            <TableColumn prefWidth="112.0" text="Name" />
            <TableColumn prefWidth="108.0" sortable="false" text="Mark" />
        </columns>
         <VBox.margin>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </VBox.margin>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</VBox>
